<ul class="todo">Restaurants to Try:
  <% _(unfinished).each(function (restaurant) { %>
    <li class="restaurant" data-id="<%=restaurant.escape("id") %>">
      <a class="list" href="#/">
        <%= restaurant.escape("name") %>
      </a>
      <input type="checkbox" <%= BellyGuide.owner ? "" : "disabled" %>>
    </li>
  <% }); %>
</ul>

<ul class="todo">Visited Restaurants:
  <% _(finished).each(function (restaurant) { %>
    <li class="restaurant" data-id="<%=restaurant.escape("id") %>">
      <a class="list" href="#/">
        <%= restaurant.escape("name") %>
      </a>
      <input type="checkbox" checked <%= BellyGuide.owner ? "" : "disabled" %>>
    </li>
  <% }); %>
</ul>

