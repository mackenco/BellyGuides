<ul>Restaurants to Try:
  <% _(unfinished).each(function (restaurant) { %>
    <li class="restaurant">
      <a href="#/restaurants/<%=restaurant.escape("id") %>">
        <%= restaurant.escape("name") %>
      </a>
    </li>
  <% }); %>
</ul>

<br>

<ul>Visited Restaurants:
  <% _(finished).each(function (restaurant) { %>
    <li class="restaurant">
      <a href="#/restaurants/<%=restaurant.escape("id") %>">
        <%= restaurant.escape("name") %>
      </a>
    </li>
  <% }); %>
</ul>

